{
  "name": "web",
  "version": "0.0.3",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && ./scripts/build.sh",
    "build:staging": "ng build --configuration=staging && ./scripts/build-staging.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "is-ci || husky install"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "~11.2.13",
    "@angular/compiler": "~11.2.13",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~11.2.13",
    "@angular/localize": "~11.2.13",
    "@angular/platform-browser": "~11.2.13",
    "@angular/platform-browser-dynamic": "~11.2.13",
    "@angular/router": "~11.2.13",
    "@angular/service-worker": "~11.2.13",
    "@ng-bootstrap/ng-bootstrap": "9.1.1",
    "bootstrap": "^4.5.0",
    "husky": "^4.2.5",
    "rxjs": "~6.6.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular-eslint/builder": "4.3.0",
