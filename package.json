{
  "name": "web",
  "version": "0.0.4",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:prod": "ng build --configuration production && ./scripts/build.sh",
    "build:staging": "ng build --configuration staging && ./scripts/build-staging.sh",
    "lint": "ng lint",
    "prepare": "is-ci || husky install",
    "compodoc": "npx compodoc -p tsconfig.doc.json -d docs",
    "compodoc:serve": "npx compodoc -p tsconfig.doc.json -s"
  },
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "~12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/localize": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@angular/service-worker": "~12.0.2",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0-beta.1",
    "bootstrap": "^4.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular-eslint/builder": "~12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/schematics": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.7.6",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "~4.25.0",
    "@typescript-eslint/parser": "~4.25.0",
    "eslint": "^7.6.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.4",
    "typescript": "~4.2.3"
  }
}
